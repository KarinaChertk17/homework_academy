#ex1
a = int(input("Введите первое число: "))
b = int(input("Введите второе число: "))
for i in range(a, b+1):
  if i % 7 == 0: #здесь проверяется кратность на 7
      print(i) #выводим из всего диапозона число, которое кратно на 7

#ex2
a = int(input("Введите первое число: "))
b = int(input("Второе число: "))
c = [i for i in range(a, b+1)]

#Функция replace () в Python позволяет произвести замену символов в строке
print("Все числа диапозона:", str(c).replace("","").replace("","")) #все числа диапозона включительно через запятую
c.sort(reverse=True) #происходит сортировка + reverse=True это cортировка  по убыванию
print("Все числа диапозона в убывающем порядке:", str(c).replace("","").replace("","")) #"","" - это чтобы числа вывоились через запятую

a = [] #некий список
b = 0

for i in c:
   if i % 7 == 0: #все числа кратные 7
       a += [i]
   if i % 5 == 0: #все числа кратные 5
       b += 1
print("Все числа кратные 7:", str(a).replace("","").replace("",""))
print("Количество чисел, кратных 5:", str(b).replace("","").replace("",""))

#ex3
a = int(input("Введите первое число: "))
b = int(input("Введите второе число: "))
for i in range(a, b+1):
   if i % 3 !=0 and i % 5 != 0:#если число кратно 3 + не равно 0 и  число кратно 5 и не равно 0 то выводится число i
       print(i)
   if i % 3 == 0 and i % 5 == 0: #если число кратно 3 и 5 то выводится Fizz Buzz
       print("Fizz Buzz")
   else:
       if i % 3 == 0: #если число кратно 3  то выводится Fizz 
           print("Fizz")
       if i % 5 == 0:#если число кратно  5 то выводится  Buzz
           print("Buzz")
